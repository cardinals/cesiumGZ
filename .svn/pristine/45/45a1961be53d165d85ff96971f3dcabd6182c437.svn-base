.jimu-widget-YuShuiQing .location-box-list {
  /*margin-left: 1%;*/
  overflow-y: auto;
  width: 100%;
  background-color: #fff;
  position: relative;
  top: 0;
  border: 1px #ccc solid;
}

.jimu-widget-YuShuiQing .location-box-list tr {
  cursor: pointer;
}

.jimu-widget-YuShuiQing .load-box-list {
  display: none;
  overflow-y: auto;
  width: 100%;
  background-color: #fff;
  position: absolute;
  top: 40px;
  bottom: 0;
  height: auto;
  border: 1px #ccc solid;
}

.jimu-widget-YuShuiQing .load-box-list .rote-load {
  position: absolute;
  top: 36%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 60px;
  height: 80px;
}

.jimu-widget-YuShuiQing .load-box-list .rote-load img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 60px;
  animation: rote 3s linear infinite;
}

.jimu-widget-YuShuiQing .load-box-list .rote-load span {
  margin: 0 !important;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 20px;
  line-height: 20px !important;
  text-align: center;
}

@keyframes rote {
  0% {
    transform: rotate(0deg);
  }

  25% {
    transform: rotate(90deg);
  }

  50% {
    transform: rotate(180deg);
  }

  75% {
    transform: rotate(270deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

