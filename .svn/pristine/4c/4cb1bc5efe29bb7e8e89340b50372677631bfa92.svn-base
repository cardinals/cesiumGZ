@import "Tooltip-div.css";
.jimu-widget-DynamicRiver{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  background-color: rgba(48, 48, 48, 0.7);
  display: none;
  font-size: 14px;
  border: 1px solid rgb(69, 154, 251);
  border-radius: 5px;
  color: rgb(238, 235, 235);
}


.jimu-widget-DynamicRiver .water-analysis {
	margin: 10px 8px; 
}
	
.jimu-widget-DynamicRiver .dynamicriver-header {
	position: relative;
	width: 100%;
	height: 40px;
	line-height: 40px;
	font-size: 20px;
	border-bottom: 1px solid rgb(69, 154, 251);
}

.jimu-widget-DynamicRiver .dynamicriver-header i {
	display: inline-block;
	margin-left: 6px;
	vertical-align: middle;
	width: 20px;
	height: 20px;
	background: url(../../../images/toolbox.png ) no-repeat 0 -20px;
	background-size: 20px;	
}

.jimu-widget-DynamicRiver .dynamicriver-header .close-dynamicriver {
	position: absolute;
	top: 50%;
	right: 8px;
	font-size: 38px;
	font-weight: 400;
	height: 28px;
	line-height: 28px;
	cursor: pointer;
	transform: translateY(-50%);
}

.jimu-widget-DynamicRiver .water-analysis ul ,
.jimu-widget-DynamicRiver .water-analysis ul li ,
.jimu-widget-DynamicRiver .water-analysis ul input {
	padding: 0;
	margin: 0;
}

.jimu-widget-DynamicRiver .water-analysis ul li {
	margin: 5px 0;
	list-style: none;
	height: 30px;
	line-height: 30px;
	font-size: 16px;
}

.jimu-widget-DynamicRiver .water-analysis ul li input {
	width: 182px;
	height: 100%;
}

.jimu-widget-DynamicRiver .water-analysis ul li span {
	display: inline-block;
	width: 100px;
}

.jimu-widget-DynamicRiver .water-analysis ul li select {
	width: 182px;
	height: 100%;
}

.jimu-widget-DynamicRiver .water-analysis .water-analysis-new {
	margin-top: 10px;
}

.jimu-widget-DynamicRiver .water-analysis .water-analysis-new input {
	width: 49%;
	height: 30px;
	color: #fff;
	background: rgb(2,24,55);
	border: 1px solid rgb(69, 154, 251);
	border-radius: 4px;
}

.jimu-widget-DynamicRiver .water-analysis .water-analysis-new div {
	margin-top: 10px;
	width: 100%;
}