.jimu-widget-HydrologicalRiskArea {
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: #fff;
}

.jimu-widget-HydrologicalRiskArea ul,
.jimu-widget-HydrologicalRiskArea li,
.jimu-widget-HydrologicalRiskArea div,
.jimu-widget-HydrologicalRiskArea input,
.jimu-widget-HydrologicalRiskArea img,
.jimu-widget-HydrologicalRiskArea table,
.jimu-widget-HydrologicalRiskArea tr,
.jimu-widget-HydrologicalRiskArea td,
.jimu-widget-HydrologicalRiskArea th,
.jimu-widget-HydrologicalRiskArea tbody,
.jimu-widget-HydrologicalRiskArea thead{
    margin: 0;
    padding: 0;
}

.jimu-widget-HydrologicalRiskArea a {
    text-decoration: none;
    color: black;
}

.jimu-widget-HydrologicalRiskArea a:hover {
    text-decoration: none;
    color: #336699;
}

.jimu-widget-HydrologicalRiskArea #Hyd_tab {
    position: absolute;
    width: 100%;
    top: 0px;
    left: 0px;
    right: 0px;
    height: 100%;
    /*margin: 1px 3px 1px 3px;*/
    /*z-index: 9;*/
}

.jimu-widget-HydrologicalRiskArea #Hyd_tab .tabul {
    height: 30px;
    line-height: 30px;
}

.jimu-widget-HydrologicalRiskArea #Hyd_tab ul {
    list-style: none;
    line-height: 30px;
    height: 30px;
}

.jimu-widget-HydrologicalRiskArea #Hyd_tab .tabul ul li {
    cursor: pointer;
    float: left;
    list-style: none;
    text-align: center;
    padding: 0px 5px;
    margin-right: 4px;
    width: 80px;
    height: 30px;
    line-height: 30px;
    background: rgb(187, 186, 186);
    border-radius: 4px;
    color: #fff;
}

.jimu-widget-HydrologicalRiskArea #Hyd_tab ul li.on {
    /*border-top: 2px solid Saddlebrown;*/
    background: #094895;
}

.jimu-widget-HydrologicalRiskArea #Hyd_tab div {
    /*width: 100%;*/
    line-height: 24px;
    /*border-top: none;*/
    /*border: 1px solid #336699;*/
}

.jimu-widget-HydrologicalRiskArea .hide {
    display: none;
}

.jimu-widget-HydrologicalRiskArea .fxq-box-list {
    /*margin-left: 1%;*/
    overflow-y: auto;
    width: 100%;
    background-color: #fff;
    position: absolute;
    top: 0;
    border: 1px #ccc solid;
}

.jimu-widget-HydrologicalRiskArea .load-box-list {
    display: none;
    overflow-y: auto;
    width: 100%;
    background-color: #fff;
    position: absolute;
    top: 0;
    border: 1px #ccc solid;
}

.jimu-widget-HydrologicalRiskArea .load-box-list .rote-load {
    position: absolute;
    top: 36%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 60px;
    height: 80px;
}

.jimu-widget-HydrologicalRiskArea .load-box-list .rote-load img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 60px;
    animation: rote 3s linear infinite;
}

.jimu-widget-HydrologicalRiskArea .load-box-list .rote-load span {
    margin: 0 !important;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 20px;
    line-height: 20px !important;
    text-align: center;
}

@keyframes rote {
    0% {
        transform: rotate(0deg);
    }

    25% {
        transform: rotate(90deg);
    }

    50% {
        transform: rotate(180deg);
    }

    75% {
        transform: rotate(270deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

