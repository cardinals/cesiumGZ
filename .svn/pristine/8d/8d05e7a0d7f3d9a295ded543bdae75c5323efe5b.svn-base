.jimu-widget-HydrologicalRiskArea .workinput {
    width: 120px;
    line-height: 20px;
    border: 1px #ddd solid;
    border-radius: 4px;
    padding: 4px;
    background-color: #fff;
    float: left;
    margin: 2px 0px;
    font-size: 14px;
}

.jimu-widget-HydrologicalRiskArea .select {
    position: relative;
    width: 88px;
    min-width: 88px;
    height: 26px;
    border: 1px solid rgba(160, 160, 156, 1);
    cursor: pointer;
    float: left;
    /*right: 10px;*/
    /*margin-top: -6px;*/
}

.jimu-widget-HydrologicalRiskArea .select:after {
    content: "";
    position: absolute;
    top: 50%;
    right: 10px;
    margin-top: -2px;
    border-top: 5px solid #666;
    border-right: 5px solid transparent;
    border-left: 5px solid transparent;
}

