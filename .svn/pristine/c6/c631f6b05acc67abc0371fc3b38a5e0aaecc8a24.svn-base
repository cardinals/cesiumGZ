.jimu-widget-HydrologicalNetwork {
	min-width: 145px;
	width: 145px;
	height: auto !important;
	position: fixed !important;
	left: 0 !important;
	top: 45px !important;
	bottom: 25px !important;
	z-index: 40 !important;
}



.jimu-widget-HydrologicalNetwork .roaming-container {
	width: 145px;
	height: 100%;
	position: absolute;
	left: 0px;
	top: 0px;
	background: rgba(146, 204, 209, 0.5);
}

.jimu-widget-HydrologicalNetwork .romm {
	height: 92%;
	overflow: hidden;
	position: relative;
}

.jimu-widget-HydrologicalNetwork .romm ul {
	padding: 0;
	margin: 0;
	height: 100%;
	position: absolute;
	left: 8px;
	top: 0;
}

.jimu-widget-HydrologicalNetwork .romm ul li {
	width: 130px;
	height: 5.9%;
	list-style: none;
}

.jimu-widget-HydrologicalNetwork .romm ul li a {
	width: 100%;
	height: 100%;
	float: left;
	position: relative;
}

.jimu-widget-HydrologicalNetwork .roam_div {
	width: 100%;
	height: 90%;
	background: rgba(4, 59, 119, .7);
	display: block;
	float: left;
	position: relative;
	color: #fff;
	cursor: pointer;
}

.jimu-widget-HydrologicalNetwork .romm ul li a div {
	width: 100%;
	height: 30px;
	position: absolute;
	top: 50%;
	margin-top: -15px;
	line-height: 30px;
}

.jimu-widget-HydrologicalNetwork .romm ul li a div img {
	margin-left: 8px;
	width: 15px;
	float: left;
	margin-top: 8px;
	border: 0px;
}

.jimu-widget-HydrologicalNetwork .romm ul li a div span {
	margin-left: 5px;
	float: left;
	font-size: 12px;
	font-family: "微软雅黑";
	position: absolute;
	height: 30px;
}

.jimu-widget-HydrologicalNetwork .prev {
	position: relative;
}

.jimu-widget-HydrologicalNetwork .prev a {
	width: 100%;
	height: 28px;
	position: absolute;
	bottom: 5px;
	background: url("../../../images/prev-up.png") no-repeat center center;
	cursor: pointer;
}

.jimu-widget-HydrologicalNetwork .next {
	position: relative;
}

.jimu-widget-HydrologicalNetwork .next a {
	width: 100%;
	height: 28px;
	position: absolute;
	top: 5px;
	background: url("../../../images/next-down.png") no-repeat center center;
	cursor: pointer;
}