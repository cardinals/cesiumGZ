.jimu-widget-ThematicAnalysisOfHydrology {
	width: 420px !important;
	height: 100%;
	position: fixed !important;
	left: 60px !important;
	top: 109px !important;
	background: rgba(59, 59, 59, 0.4);
	z-index: 41 !important;
	color: #fff;
}

.jimu-widget-ThematicAnalysisOfHydrology div,
.jimu-widget-ThematicAnalysisOfHydrology ul,
.jimu-widget-ThematicAnalysisOfHydrology li,
.jimu-widget-ThematicAnalysisOfHydrology span,
.jimu-widget-ThematicAnalysisOfHydrology input,
.jimu-widget-ThematicAnalysisOfHydrology h2 {
	margin: 0;
	padding: 0;
}

.jimu-widget-ThematicAnalysisOfHydrology div.dong {
	height: 100%;
	padding: 10px 20px;
}

.jimu-widget-ThematicAnalysisOfHydrology ul {
	height: 100%;
}

.jimu-widget-ThematicAnalysisOfHydrology li {
	list-style: none;
	height: 50px;
	/* text-align: center; */
	line-height: 50px;
	font-size: 14px;
}

.jimu-widget-ThematicAnalysisOfHydrology li span {
	display: inline-block;
	width: 50%;
	text-align: left;
	margin-left: 5%;
}

.jimu-widget-ThematicAnalysisOfHydrology li input {
	display: inline-block;
	width: 50%;
	height: 40px;
	border: 0;
	border-radius: 8px;
	color: #fff;
	background: rgba(116, 116, 116, 0.7);
	padding: 0 10px;
}

.jimu-widget-ThematicAnalysisOfHydrology li input:nth-child(5) {
	border: 1px solid skyblue;
}

.jimu-widget-ThematicAnalysisOfHydrology li:nth-child(1) input,
.jimu-widget-ThematicAnalysisOfHydrology li:nth-child(5) input {
	cursor: pointer;
}

.jimu-widget-ThematicAnalysisOfHydrology .dong-echarts {
	display: none;
	position: absolute;
	top: 360px;
	background: rgba(90, 87, 87, 0.7);
	width: 560px;
	height: 360px;
	z-index: 55;
}

.jimu-widget-ThematicAnalysisOfHydrology p {
	display: none;
	margin: 0;
	padding: 0;
	position: absolute;
	top: 368px;
	left: 12px;
	width: 150px;
	height: 200px;
	color: #fff;
	line-height: 20px;
	text-indent: 2em;
	letter-spacing: 1.5px;
	font-size: 16px;
	word-break: break-all;
	text-align-last: left;
	text-align: justify;
	text-justify: distribute-all-lines;
	z-index: 56;
}

.jimu-widget-ThematicAnalysisOfHydrology .dong-progress {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	width: 500px;
	height: 0px;
	z-index: 33;
	background: rgba(59, 59, 59, 0.3);

}

.jimu-widget-ThematicAnalysisOfHydrology .dong-progress h2 {
	margin: 10px 0;
	text-align: center;
	font-size: 16px;
	font-weight: 400;
}

.jimu-widget-ThematicAnalysisOfHydrology .dong-progress .container {
	display: inline-block;
	width: 100%;
	height: 20px;
	padding-right: 10px;
	border: 1px solid #999;
	border-radius: 5px;
}

.jimu-widget-ThematicAnalysisOfHydrology .dong-progress .container .h-100P {
	height: 100%;
}

.jimu-widget-ThematicAnalysisOfHydrology .dong-progress .container .bar {
	display: inline-block;
	background: linear-gradient(to right, #ff4343, #48adff);
	color: white;
	font-weight: bold;
	padding: 0 5px;
	text-align: right;
	border-radius: 5px;
	border-right: 1px solid #999;
}



.jimu-widget-ThematicAnalysisOfHydrology .storage-capacity-curve {
	display: none;
	padding-top: 10px; 
	position: absolute;
	top: 360px;
	left: 580px;
	background: rgba(206, 204, 204, 0.7);
	width: 500px;
	height: 360px;
	z-index: 55;
}

.jimu-widget-ThematicAnalysisOfHydrology .storage-name {
	display: none;
	position: absolute;
	top: 686px;
	left: 780px;
	width: 100px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	font-size: 14px;
	color: rgba(40, 37, 161, 0.849);
	z-index: 111 !important;
}