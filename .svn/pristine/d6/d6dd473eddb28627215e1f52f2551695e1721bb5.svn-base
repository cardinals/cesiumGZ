.jimu-widget-HydrologicalNetwork {
  position: relative;
  width: 100%;
  height: 100%;
  /* overflow: auto; */
  background: #fff;
}

.jimu-widget-HydrologicalNetwork a {
  text-decoration: none;
}

.jimu-widget-HydrologicalNetwork ul,
.jimu-widget-HydrologicalNetwork li,
.jimu-widget-HydrologicalNetwork div,
.jimu-widget-HydrologicalNetwork input,
.jimu-widget-HydrologicalNetwork img {
  margin: 0;
  padding: 0;
  list-style: none;
}

.jimu-widget-HydrologicalNetwork .hydronetwork {
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
  right: 0;
  height: 100%;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworktab ul li {
  margin: 4px 1%;
  width: 31.3%;
  height: 30px;
  line-height: 30px;
  cursor: pointer;
  float: left;
  text-align: center;
  background: rgb(187, 186, 186);
  border-radius: 4px;
  color: #fff;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworktab ul li.on {
  background: #094895;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hide {
  display: none;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist {
  position: absolute;
  top: 76px;
  bottom: 0;
  width: 100%;
  overflow-y: auto;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist > div {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .location-box-list {
  position: relative;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 85%;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .net-work-query {
  height: 40px;
  line-height: 40px;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .net-work-query input[type="text"] {
  margin-right: 1%;
  padding: 2%;
  vertical-align: middle;
  width: 78%;
  height: 30px;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .net-work-query input[type="button"] {
  vertical-align: middle;
  height: 30px;
  width: 20%;
  cursor: pointer;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .location-box-list th,
.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .location-box-list td {
  text-align: center;
}

.jimu-widget-HydrologicalNetwork .hydronetwork .hydroworklist .location-box-list .dgrid-column-ind {
  width: 60px;
}

/* 加载状态 */
.jimu-widget-HydrologicalNetwork .net-work-loadbox {
  display: none;
  overflow-y: auto;
  width: 100%;
  background-color: #fff;
  position: absolute;
  top: 40px;
  bottom: 0;
  height: auto;
  border: 1px #ccc solid;
}

.jimu-widget-HydrologicalNetwork .net-work-loadbox .net-work-roteload {
  position: absolute;
  top: 36%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 60px;
  height: 80px;
}

.jimu-widget-HydrologicalNetwork .net-work-loadbox .net-work-roteload img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 60px;
  animation: rote 3s linear infinite;
}

.jimu-widget-HydrologicalNetwork .net-work-loadbox .net-work-roteload span {
  margin: 0 !important;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 20px;
  line-height: 20px !important;
  text-align: center;
}

@keyframes rote {
  0% {
    transform: rotate(0deg);
  }

  25% {
    transform: rotate(90deg);
  }

  50% {
    transform: rotate(180deg);
  }

  75% {
    transform: rotate(270deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.jimu-widget-HydrologicalNetwork .location-box-list tr {
  height: 23px;
  line-height: 23px;
}

